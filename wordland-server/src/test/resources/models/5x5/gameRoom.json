[
  {
    "name": "5x5",
    "template": true,
    "settings": {
      "board": { "name": "5x5" },
      "maxPlayers": 2,
      "turnPolicies": [
        {
          "name": "round_robin",
          "type": "round_robin",
          "params": [{"name": "max_turn_duration", "value": "5d"}]
        }
      ],
      "missedTurnPolicy": "forfeit_game",
      "winConditions": [{
        "name": "controls_board",
        "defaultMessage": "The winner has the most tiles after all tiles have been played",
        "endJs": "board.blankCount() == 0",
        "winnerJs": "board.playersByCount.entrySet.iterator().next().id"
      }]
    }
  }
]